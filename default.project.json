{
	"name": "mathcat",
	"emitLegacyScripts": false,
	"globIgnorePaths": ["**/package.json", "**/tsconfig.json"],
	"tree": {
		"$className": "DataModel",
		"ReplicatedStorage": {
			"$className": "ReplicatedStorage",
			"rbxts_include": {
				"$path": "include",
				"node_modules": {
					"$className": "Folder",
					"@rbxts": {
						"$path": "node_modules/@rbxts"
					},
					"@rbxts-js": {
						"$path": "node_modules/@rbxts-js"
					}
				}
			},
			"mathcat": {
				"$path": "out/src"
			},
			"tst": {
				"$path": "out/tst"
			}			
		}
	}
}